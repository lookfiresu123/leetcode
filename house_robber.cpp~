#include <vector>
#include <iostream>
#include <cmath>
#include <ctime>
#include <cstdlib>

using namespace std;

class Solution {
public:
    int most(vector<int>::iterator begin, vector<int>::iterator end) {
        if (begin >= end)
            return 0;
        else if (begin + 1 == end)
            return *begin;
        else
            return std::max(*begin + most(begin + 2, end), *(begin+1) + most(begin + 3, end));
    }

    // rob(a1~an) = Max { a1+rob(a3~an), a2+rob(a4~an) }
    int rob(vector<int> &nums) {
        return most(nums.begin(), nums.end());
    }
};

int main () {
    vector<int> nums;
    Solution sol;
    std::srand(std::time(nullptr));
    for (int i = 0 ; i < 10 ; ++i)
        nums.push_back(std::rand() % 100);
    for (auto i : nums)
        cout << i << " ";
    cout << endl;
    cout << sol.rob(nums) << endl;
    return 0;
}
